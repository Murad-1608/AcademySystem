@model List<StudentManagementSystem.Entities.StudentCourse>
@{
    ViewData["Title"] = "Tələbələr";
}
<h2>@Model[0].Course.Name kursunun tələbələri</h2>
<table id="example" class="table table-striped">
    <thead>
        <tr>
            <th>№</th>
            <th>Id</th>
            <th>Ad</th>
            <th>Soyad</th>
            <th>Qrup</th>
            <th>İxtisas</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i].Student.Id</td>
                <td>@Model[i].Student.Name</td>
                <td>@Model[i].Student.Surname</td>
                <td>@Model[i].Student.Group</td>
                <td>@Model[i].Student.Specialty</td>
            </tr>
        }
    </tbody>
</table>